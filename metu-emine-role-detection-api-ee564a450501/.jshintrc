{
    "maxdepth": 4,
    "maxstatements": 200,
    "maxcomplexity": 16,
    "maxlen": 120,
    "maxparams": 6,

    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": false,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "undef": true,
    "unused": "vars",
    "trailing": true,

    "quotmark": false,
    "expr": true,
    "asi": true,

    "browser": false,
    "esnext": true,
    "devel": false,
    "node": true,
    "nonstandard": false,

    "predef": ["require", "module", "__dirname", "__filename", "phantom",
        "document", "Node", "traverseDOMTree", "window", "describe", "it"]
}
